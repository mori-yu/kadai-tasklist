<% if logged_in? %>
  <h1>タスク一覧</h1>
  <ul>
    <% @tasks.each do |task| %>
      <li>
      	<%= link_to task.id, task %> : 
      	<%= task.status %> >
      	<%= task.content %>
      </li>
    <% end %>
  </ul>
<% else %>
  <%= link_to 'Sign up now!', signup_path, class: 'btn btn-lg btn-primary' %>
<% end %>